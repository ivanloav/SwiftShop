{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NestJS",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/backend/src/main.ts",
      "autoAttachChildProcesses": true,
      "sourceMaps": true,
      "restart": true,
      "console": "integratedTerminal"
    }
  ]
}
